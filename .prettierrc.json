{
  "singleQuote": true,
  "semi": false,
  "tabWidth": 2,
  "trailingCommas": "es5",
  "printWidth": 120,
  "useTabs": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^[./]",
    "(~/components/)",
    "~/screens/",
    "~/contexts/",
    "~/hooks/",
    "~/models/",
    "~/libs/",
    "~/utils/",
    "~/services",
    "(.css)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
